{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sku": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "vendor": {
      "type": "string"
    },
    "vendorCode": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "topBottom": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "rise": {
      "type": "string"
    },
    "subcategory": {
      "type": "string"
    },
    "shortName": {
      "type": "string"
    },
    "fitBottom": {
      "type": "string"
    },
    "season": {
      "type": "string"
    },
    "seasonality": {
      "type": "string"
    },
    "composition": {
      "type": "string"
    },
    "waist": {
      "type": "string"
    },
    "categoryParam": {
      "type": "string"
    },
    "manufaktura": {
      "type": "string"
    },
    "denomination": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "products"
    },
    "slug": {
      "type": "uid"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "product"
    },
    "product_variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant.product-variant",
      "mappedBy": "product"
    },
    "hit": {
      "type": "boolean"
    },
    "sale": {
      "type": "biginteger"
    }
  }
}
